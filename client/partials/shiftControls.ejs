<div class="shiftControls">
  <div role="group" class="shiftControlsPositionGroup">
    <button
      class="secondary outline shiftControlButton"
      @click="moveShift(shift.id, -1)"
      data-tooltip="Change order"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        viewBox="0 0 12 12"
        width="12"
        height="12"
      >
        <path
          d="M6 4c-.2 0-.4.1-.5.2L2.2 7.5c-.3.3-.3.8 0 1.1.3.3.8.3 1.1 0L6 5.9l2.7 2.7c.3.3.8.3 1.1 0 .3-.3.3-.8 0-1.1L6.6 4.3C6.4 4.1 6.2 4 6 4Z"
        ></path>
      </svg>
    </button>
    <button
      class="secondary outline shiftControlButton"
      @click="moveShift(shift.id, 1)"
      data-tooltip="Change order"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        viewBox="0 0 12 12"
        width="12"
        height="12"
      >
        <path
          d="M6 8.825c-.2 0-.4-.1-.5-.2l-3.3-3.3c-.3-.3-.3-.8 0-1.1.3-.3.8-.3 1.1 0l2.7 2.7 2.7-2.7c.3-.3.8-.3 1.1 0 .3.3.3.8 0 1.1l-3.2 3.2c-.2.2-.4.3-.6.3Z"
        ></path>
      </svg>
    </button>
  </div>
  <div></div>
  <div class="shiftName">
    {{ shift.name }}
  </div>
  <div></div>
  <div class="shiftControlsRight">
    <button
    class="secondary outline shiftControlButton"
    @click="signOut(shift.id)"
    data-tooltip="Sign Off"
  >
    Off
  </button>
     <%- include('./assignPopoverMini.ejs');
    %>
  </div>

</div>
